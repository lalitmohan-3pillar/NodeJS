{
    "plugins": ["plugins/markdown"],
    "recurseDepth":10,
    "opts": {
        "destination": "docs",
        "recurse": true,        
        "template": "node_modules/docdash"
      },    
    "source": {
      "include": ["./","Readme.md"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "node_modules"
    },
    "sourceType":"module",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },    
    "templates": {
      "cleverLinks":false,
      "monospaceLinks":false
    },
    "docdash":{
        "sort":true,
        "search":true,
        "collapse":false
    }
    
}
