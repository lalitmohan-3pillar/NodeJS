<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="/static/css/card.css" />
    <link rel="stylesheet" type="text/css" href="/static/css/product.css" />
{{>navbar data=details}}
<script>
  
  function searchCategoryWise(id){    
    fetch("/assessment/filterProduct", {
      method: "POST",
      body: JSON.stringify({
                productCategory: id
                        }),
      headers: {
                "Content-type": "application/json; charset=UTF-8"
              }
}).then((response) => {
      response.json()  
      //console.log(response)
      response.redirected('/assessment/addproduct')
      //window.location.href = '/assessment'      
                 });
  }
</script>
</head>
<body >
<div class="row" >
  <div class="col-md-1 offset-md-1">
    <div class="category">
      <a   href="/assessment/filterProduct?productCategory=All">
        <img class ="img-category" src="data:image/jpeg;base64,">
        <h6>All </h6>
      </a>
    </div>
  </div>
  <div class="col-md-1 offset-md-1">
    <div class="category">
      <a  href="/assessment/filterProduct?productCategory=Mens">
        <img  class ="img-category" src="https://rukminim1.flixcart.com/flap/128/128/image/c12afc017e6f24cb.png?q=100" alt="">
        <h6>Mens</h6>
      </a>
    </div>
  </div>
  <div class="col-md-1 offset-md-1">
    <div class="category">
      <a  href="/assessment/filterProduct?productCategory=Womens">
        <img class ="img-category" src="data:image/jpeg;base64," alt="">
        <h6>Womens</h6>
      </a>
    </div>
  </div>
    <div class="col-md-1 offset-md-1">
      <div class="category">
      <a  href="/assessment/filterProduct?productCategory=Kids">
        <img class ="img-category" src="https://rukminim1.flixcart.com/flap/128/128/image/dff3f7adcf3a90c6.png?q=100" alt="">
        <h6>Kids</h6>
      </a>
    </div>
  </div>
</div>

<div style="padding-top:5%" >
    
  {{#if product}}
 
    {{#each product}}
    <div class="cards"> 
      <div class='card'>
        <a href="/assessment/productDetail?productName={{this._doc.productName}}">
          <img class='card__img' src={{this._doc.productUrl}} /> 
        </a>
        
        
        <div class="card__info" >
          <p class="card__category"> <strong>{{this._doc.productName}}  </strong>  </p>
            <p class="card__title">
            
                <strong>Price:</strong>  {{this._doc.productPrice}}              
            </p>         
          <p class="card__title"> {{this.description }} ...</p>
          <div class="d-flex justify-content-between align-items-center">
            <div class="btn-group">

              <a type="button" class="btn btn-sm btn-primary product-btn" 
              href="/assessment/addToCartByProduct?productName={{this._doc.productName}}">Add to Cart</a>

              <a type="button" class="btn btn-sm btn-outline-secondary product-btn" 
                href="/assessment/productDetail?productName={{this._doc.productName}}">View Details</a>

                <a type="button" class="btn btn-sm btn-primary product-btn" 
                href="/assessment/editProduct?productName={{this._doc.productName}}">Edit</a>
            </div>
          
          </div>
        </div> 
      </div>
  </div>  
   {{/each}}
   {{else}}
    <div class="alert alert-danger mx-5 text-center" >
          Nothing to show  , Please add the Product
    </div>
   {{/if}}
    <a  class="btn btn-sm btn-primary " href="/assessment/addproduct" style="margin:20">Add Product</a>

</div>
{{>footer}}
</body>
</html>